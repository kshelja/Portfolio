<!DOCTYPE html>
<html>

<head>
  <title>
    <%= title %>
  </title>
  <%- include partials/headlinks %>
</head>

<body>

  <!-- Partial header included below -->
  <%- include partials/header %>




    <section style="background-color: #e9ecefb3;">

      <div class="fullheight pb-5">


        <div class="container">
          <div class="row align-items-stretch justify-content-center no-gutters">
            <div class="col-md-7">
              <div class="form h-100 contact-wrap p-5">
                <h1 class="mb-5">Let's Talk</h1>
                <h3 class="mb-2">Name - Kumari Shelja</h3>
                <h3 class="mb-2">E-Mail - kumarisheljaa@gmail.com</h3>
                <h3 class="mb-5">Contact No - 1234567890</h3>
                <form class="mb-5 bg-white p-3" id="formsubmitted" name="contactForm">
                  <div class="row">
                    <div class="col-md-6 form-group mb-3">
                      <label for="" class="col-form-label">First Name *</label>
                      <input type="text" class="form-control" name="firstname" id="firstname" placeholder="First name">
                    </div>
                    <div class="col-md-6 form-group mb-3">
                      <label for="" class="col-form-label">Last Name *</label>
                      <input type="text" class="form-control" name="lastname" id="lastname" placeholder="Last name">
                    </div>
                    <div class="col-md-6 form-group mb-3">
                      <label for="" class="col-form-label">Contact Number *</label>
                      <input type="text" class="form-control" name="contactnumber" id="contactnumber"
                        placeholder="Your contact number">
                    </div>
                    <div class="col-md-6 form-group mb-3">
                      <label for="" class="col-form-label">Email *</label>
                      <input type="email" class="form-control" name="email" id="email" placeholder="Your email">
                    </div>
                  </div>
                  <div class="row">
                    <div class="col-md-12 form-group mb-3">
                      <label for="budget" class="col-form-label">Subject</label>
                      <input type="text" class="form-control" name="subject" id="subject" placeholder="Your subject">
                    </div>
                  </div>
                  <div class="row mb-5">
                    <div class="col-md-12 form-group mb-3">
                      <label for="message" class="col-form-label">Message *</label>
                      <textarea class="form-control" name="message" id="message" cols="30" rows="4"
                        placeholder="Write your message"></textarea>
                    </div>
                  </div>
                  <div class="row justify-content-center">
                    <div class="col-md-5 form-group text-center">
                      <input type="submit" value="Send Message" class="btn btn-block btn-primary rounded-0 py-2 px-4">
                      <span class="submitting"></span>
                    </div>
                  </div>
                </form>
              </div>
            </div>
          </div>
        </div>

      </div>


    </section>









    <!-- Partial Footer included below -->
    <%- include partials/footer %>



</body>

</html>